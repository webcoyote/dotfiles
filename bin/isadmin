#!/usr/bin/env bash
set -Eeuo pipefail

function isadmin() {
    case "$OSTYPE" in
        msys*|cygwin*)
            if ! net session &>/dev/null ; then
                return 1 # user
            else
                return 0 # admin
            fi
        ;;

        *)
            if [[ $(id -u) -ne 0 ]]; then
                return 1 # user
            else
                return 0 # admin
            fi
        ;;
    esac
}

if isadmin ; then
    echo "Administrator"
else
    echo "User"
fi
